<div class="modal fade" id="myModa1packageEnquiry" tabindex="-1" role="dialog" aria-labelledby="myModalLabel" aria-hidden="true">
    <div class="modal-dialog modal-lg">

        <form action="https://www.indoasiaholidays.com/tour-packages/send-enquiry" method="post" class="package_enquiry form-pop" autocomplete="off">

            <div class="modal-content ">

                <div class="modal-header label-primary popup-header-count">
                    <h4>Send Enquiry for <span id="packagenamemodal"></span></h4>
                    <button type="button" class="close" data-dismiss="modal" aria-hidden="true" style="margin-top:3px; padding-left:4px"><i class="fa fa-times" aria-hidden="true"></i></button>
                </div>
                <div class="modal-body mob-pad">

                    <div class="animate-box">


                        <div class="panel-body">

                            <div class="package_det_d_left_form no-padding successmsg">

                                <div id="displaySendEnquiryForm">
                                    <input type="hidden" name="mpackage_name" id="mpackage_name" value="" />
                                    <input type="hidden" name="leadsend" id="leadsend" value="0" />
                                    <input type="hidden" name="modified" id="package_modified" value="0" />
                                    <input type="hidden" name="package_sid" id="package_sid" value="" />
                                    <input type="hidden" name="package_tpid" id="package_tpid" value="" />
                                    <input type="hidden" name="packagesys_id" id="packagesys_id" value="" />
                                    <input type="hidden" name="packagetype_id" id="packagetype_id" value="" />
                                    <input type="hidden" name="package_tourtype" id="package_tourtype_id" value="" />
                                    <input type="hidden" name="package_hotelcategoryid" id="package_hotelcategoryid" value="" />
                                    <input type="hidden" name="package_mealplantype" id="package_mealplantypeid" value="" />
                                    <input type="hidden" name="packagedesname" id="packagedesname" value="" />

                                    <input type="hidden" id="itinerary_inputs" value="1" />
                                    <input type="hidden" id="itinerary_rooms" value="2" />

                                    <div class="row">
                                        <div class="col-lg-12">
                                            <p class="msg" style="color:#ff0000; text-align:center; display: none;"></p>
                                        </div>

                                        <div class="col-md-4 form-group">
                                            <label class="Lable" for="name">Traveler Name <strong class="text-danger">*</strong></label>
                                            <input type="text" name="name" class="en_customername inputBox form-control" placeholder="Enter Name" aria-label="Enter Name" maxlength="50" value="" />
                                        </div>

                                        <div class="col-md-4 form-group">
                                            <label for="email" class="Lable">Email <strong class="text-danger">*</strong></label>
                                            <input type="text" name="email" class="en_customeremail inputBox form-control notSpace" placeholder="Enter Email" aria-label="Enter Email" maxlength="50" value="" />
                                        </div>

                                        <div class="col-md-4 form-group">
                                            <label class="Lable">Mobile <strong class="text-danger">*</strong></label>
                                            <input type="text" name="mobile" class="en_customermobile inputBox form-control" placeholder="Enter Mobile" aria-label="Enter Mobile" maxlength="10" value="" />
                                        </div>

                                    </div>
                                </div>


                                <!-- enquiry detail form here -->
                                <!--<div id="" style="display: block;" >-->
                                <div id="expandsave" style="display: block;">

                                    <div class="col-md-12 no-padding">
                                        <h4>Enquiry Details</h4>
                                    </div>


                                    <div class="row">

                                        <div class="col-md-6 form-group">
                                            <label for="from_destination" class="Lable">Departure City <strong class="text-danger"></strong></label>
                                            <input type="text" name="from_destination" class="en_customerfromdestination inputBox form-control" placeholder="Departure City" aria-label="Enter destination From" maxlength="50" />
                                            <input type="hidden" name="from_destination_id" id="from_destination_id" maxlength="50" class="from_destination_id" />
                                        </div>

                                        <div class="col-md-6 form-group">
                                            <label for="date" class="Lable">Start Date <strong class="text-danger"></strong></label>
                                            <input type="text" name="date" class="en_customertripdate txtField datepicker form-control" placeholder="From Date" aria-label="From Date" maxlength="10" />
                                        </div>

                                        <div class="cl"></div>


                                        <!-- Room Start -->

                                        <div class="room-rows-container">

                                            <div class="col-md-12 form-group">
                                                <label>Room 1</label>
                                                <input type="hidden" name="room[]" value="1" />
                                            </div>


                                            <div class="col-md-2 form-group">
                                                <label class="Lable">Adults(12+ Yrs)</label>
                                                <select name="adult[]" class="traveler_adult form-control" onchange="checkTravellers(this , 1);" id="traveler_adult_1">
                                                    <option value="1">1</option>
                                                    <option value="2">2</option>
                                                    <option value="3">3</option>
                                                </select>
                                            </div>

                                            <div class="col-md-2 form-group room-row-adult-bedtype-1" style="display: none" id="room-row-adult-bedtype-1">
                                                <label class="Lable">Adult Bedtype</label>
                                                <select name="adult_bed_type[]" class="traveler_child form-control room-row-adult-bedtype-1-1" id="room-row-adult-bedtype-1-1" onchange="checkAdultBedType(this);">
                                                    <option value="">Select</option>
                                                    <option value="withoutbed">Without Bed</option>
                                                    <option value="extrabed">With Bed</option>
                                                </select>
                                            </div>
                                            <div class="col-md-2 form-group">
                                                <label class="Lable">Kids(2 - 12Yrs)</label>
                                                <select name="child[]" class="traveler_child form-control" id="traveler_kids_1" onchange="checkTravellers(this,1);">
                                                    <option value="0"> 0</option>
                                                    <option value="1">1</option>
                                                    <option value="2">2</option>
                                                </select>
                                            </div>

                                            <div class="col-md-2 form-group room-row-child-bedtype-1 room-row-child-bedtype-11" style="display: none" id="room-row-child-bedtype-11">
                                                <label class="Lable">Child 1 Bedtype</label>
                                                <select name="child1_bed_type[]" class="traveler_child form-control room-row-child-bedtype-1-1" id="room-row-child-bedtype-1-1" onchange="checkAdultBedType(this);">
                                                    <option value="">Select</option>
                                                    <option value="withoutbed">Without Bed</option>
                                                    <option value="extrabed" selected>With Bed</option>
                                                    <option value="none">None</option>
                                                </select>
                                            </div>
                                            <div class="col-md-2 form-group room-row-child-bedtype-1 room-row-child-bedtype-12" style="display: none" id="room-row-child-bedtype-12">
                                                <label class="Lable">Child 2 Bedtype</label>
                                                <select name="child2_bed_type[]" class="traveler_child form-control room-row-child-bedtype-1-2" id="room-row-child-bedtype-1-2" onchange="checkAdultBedType(this);">
                                                    <option value="">Select</option>
                                                    <option value="withoutbed">Without Bed</option>
                                                    <option value="extrabed" selected>With Bed</option>
                                                    <option value="none">None</option>
                                                </select>
                                            </div>

                                            <div class="col-md-2 form-group">
                                                <label class="Lable">Infant(0 - 2Yrs)</label>
                                                <select name="infant[]" class="traveler_infant form-control" id="traveler_infant_1" onchange="checkTravellers(this,1);">
                                                    <option value="0"> 0</option>
                                                    <option value="1">1</option>
                                                    <option value="2">2</option>
                                                    <option value="3">3</option>
                                                    <option value="4">4</option>
                                                </select>
                                            </div>

                                        </div>

                                        <!-- Room End -->



                                        <div class="cl"></div>



                                        <div class="col-lg-12">
                                            <div class="row">
                                                <div class="col-lg-2 block">
                                                    <a href="javascript:void(0);" class="btn btn-danger btn-sm addmore pull-left">+Add Room</a>
                                                </div>

                                            </div>
                                        </div>
                                        <div class="cl mt-3"></div>

                                        <!--button added here-->



                                        <div class="col-lg-12 alert" id="tableCost" style="margin-bottom:0px; display: none">

                                            <div class="cl"></div>
                                            <div class="mb-6"></div>

                                            <div class="row">

                                                <div class="col-md-12 mob-pad-tab">
                                                    <div class="col-md-4" id="totalBasicCostTR">
                                                        <div class="alert alert-success btn-outline1">
                                                            Total Basic Cost: <span class="WebRupee">Rs</span> <span id="totalBasicCost">0</span>
                                                        </div>
                                                    </div>
                                                    <div class="col-md-3" id="totalTaxCostTR">
                                                        <div class="alert alert-success btn-outline1">
                                                            GST: <span class="WebRupee">Rs</span> <span id="totalTaxCost">0</span>
                                                        </div>
                                                    </div>
                                                    <div class="col-md-5" id="totalGrandCostTR">
                                                        <div class="alert alert-success btn-outline1">
                                                            <div class="pric">
                                                                Grand Total: <span class="WebRupee">Rs</span> <span id="totalGrandCost">0</span>
                                                            </div>
                                                        </div>
                                                    </div>

                                                </div>
                                            </div>

                                        </div>

                                    </div>

                                </div>



                            </div>

                        </div>

                    </div>

                    <div class="clear"></div>

                </div>



                <div class="modal-footer alert-dismissible white hidebutton" align="center">

                    <div class="col-md-12">
                        <div class="row">
                            <div class="col-md-12">
                                <p class="smsg "></p>
                            </div>
                            <div class="col-md-12 mob-pad">
                                <!--<button type="button" class="btn btn-default btn-sm btn-small-1" data-dismiss="modal">Close</button>-->
                                <div align="center"><button class="btn btn-danger btn-lg sending" type="submit" style="padding:10px 20px !important;">Submit</button></div>

                                <a style="cursor: pointer; display: none;" class="btn-link class-modify-enquiry" onclick="modify_enquiry(this);">Modify</a>
                                <div align="center"><input type="checkbox" name="authoriseme" id="authoriseme" value="1" class="icheck-green-" style="" checked>&nbsp;<span style="font-size: 11px;"> I hereby accept the <a class="text-danger" href="privacy-policy.html" target="_blank">Privacy Policy</a> and authorize Sayonestop Holidays and its representatives to contact me.</span></div>
                            </div>
                        </div>
                    </div>

                </div>


            </div>

        </form>

    </div>
</div>